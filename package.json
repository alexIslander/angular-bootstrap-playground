{
  "name": "angular-bootstrap-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage",
    "build:ci": "ng build --configuration production",
    "test:ci": "jest --ci",
    "watch": "ng build --watch --configuration development",
    "lint": "npm run lint-ts && npm run lint-scss",
    "lint-ts": "ng lint",
    "lint-ts-fix": "ng lint --fix",
    "lint-scss": "stylelint **/*.scss",
    "lint-scss-fix": "stylelint **/*.scss --fix",
    "prod-build-lint": "npm run build:ci && npm run lint-ts && npm run lint-scss && npm run test:ci"
  },
  "lint-staged": {
    "src/**/*.scss": "npm run lint-scss",
    "!(src/app/app.component.html),src/app/**/*.{ts,html}": [
      "npm run lint-ts",
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/bootstrap": "^5.2.6",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-preset-angular": "^13.1.4",
    "lint-staged": "^13.2.3",
    "micromatch": "^4.0.5",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.1.6"
  }
}
